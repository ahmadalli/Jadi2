{
    "name": "Jadi2",
    "version": "1.2.2.0",
    "manifest_version": 2,
    "description": "Redirects jadi.net and freekeyboard.net to jadi2.undo.it",
    "homepage_url": "http://ahmadalli.net",
    "permissions": [
        "webRequest",
        "<all_urls>",
        "webRequestBlocking"
    ],
    "background": {
        "scripts": [ "background.js" ]
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}
